    "prepare": "webpack",
    "watch": "watch 'npm run prepare' lib/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "webpack": "^3.12.0"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.0.0",
    "lodash": "^4.17.19",
    "mem": "^5.1.1",
    "msa": "^1.0.3"
  },
  "jupyterlab": {
    "extension": "lib/labplugin"
  }
}
