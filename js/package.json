{
  "name": "ipymsa",
  "version": "0.1.0",
  "description": "Jupyter Widget for MSA (Multiple Sequence Alignment) Viewer",
  "author": "Ricky Lim",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com//ipymsa.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "scripts": {
    "prepublish": "webpack",
    "build": "webpack",
    "clean": "rimraf dist/ && rimraf ../ipymsa/static/",
